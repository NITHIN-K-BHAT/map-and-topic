     <int-org-entry id="SL607781" print-style="COLUMN" sllink="SLSDOC" slsdocrf="INT-ORG-ENTRYREF">
          <int-org-entry-head>
               <int-org-name-group type="MAIN">
                    <org-entry-name>United Nations Institute for Training and Research</org-entry-name>
                    <abbr>UNITAR</abbr>
               </int-org-name-group>
          </int-org-entry-head>
          <int-org-block>
