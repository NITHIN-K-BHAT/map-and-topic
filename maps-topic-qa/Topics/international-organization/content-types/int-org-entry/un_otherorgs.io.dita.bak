          <int-org-entry id="IO-CASEE.667" print-style="COLUMN" sllink="SLSDOC" slsdocrf="INT-ORG-ENTRYREF">
               <int-org-entry-head>
                    <int-org-name-group type="MAIN">
                         <org-entry-name>International Civil Aviation Organization</org-entry-name>
                         <abbr>ICAO</abbr>
                    </int-org-name-group>
               </int-org-entry-head>
               <int-org-block>
                    <address-block>
                         <address type="POSTAL">
                              <verbatim-address>999 University St, Montréal, QC H3C 5H7, Canada</verbatim-address>
                         </address>
