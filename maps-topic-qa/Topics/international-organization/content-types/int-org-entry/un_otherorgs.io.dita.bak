     <int-org-entry id="UN.IO.MICT.1" print-style="COLUMN" sllink="SLSDOC" slsdocrf="INT-ORG-ENTRYREF" usage="casee ass">
          <int-org-entry-head>
               <int-org-name-group type="MAIN">
                    <org-entry-name>International Residual Mechanism for International Criminal Tribunals </org-entry-name>
                    <abbr>IRMCT</abbr>
               </int-org-name-group>
          </int-org-entry-head>
          <int-org-block>
               <address-block>
                    <address type="POSTAL">
