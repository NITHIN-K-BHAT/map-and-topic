          <int-org-entry id="UN_OTHERORGS.WTO.1" print-style="COLUMN" sllink="SLSDOC" slsdocrf="INT-ORG-ENTRYREF">
               <int-org-entry-head>
                    <int-org-name-group type="MAIN">
