                         <org-entry-head>
                              <org-entry-name>International Seabed Authority</org-entry-name>
                              <abbr>ISA</abbr>
                         </org-entry-head>
                         <address-block>
                              <address type="POSTAL">
                                   <verbatim-address>14-20 Port Royal St, Kingston, Jamaica</verbatim-address>
                              </address>
