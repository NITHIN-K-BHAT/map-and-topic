                    <org-entry id="WB0231XH.IO.TXT.200.1" media="PRINTANDONLINE" print-only="NO" sllink="SLSDOC" slsdocrf="ORG-ENTRYREF" usage="eerca">
                         <org-entry-head>
                              <org-entry-name>International Olympic Committee (IOC)</org-entry-name>
                         </org-entry-head>
                         <address-block>
                              <address type="POSTAL">
                                   <verbatim-address>Chãteau de Vidy, 1001 Lausanne, Switzerland</verbatim-address>
                              </address>
